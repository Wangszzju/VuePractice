{
  "parser":"babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": 1,
    "arrow-body-style": 0,
    "no-new": 0,
    "no-unused-expressions": 0,
    "no-throw-literal": 0,
    "generator-star-spacing": 0
  },
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "plugins": [
    "vue",
    "html"
  ]

}
